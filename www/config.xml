<widget id="com.sumcousa.epidashboard" version="1.0.025" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
  <name short="Epi_Dashboard">SUMCO-SPQ-Epi_Dashboard</name>
  <description>
    SUMCO-SPQ-Epi_Dashboard Rev 1.0.025
  </description>
  <author email="support@sumcousa.com" href="http://www.sumcousa.com/location_albuq.htm">
